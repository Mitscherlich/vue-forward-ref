{
  "name": "v-forward-ref",
  "version": "0.1.2",
  "description": "Portal `React.forwardRef` api into Vue",
  "author": "Mitscherlich <mitscherlich36@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Mitscherlich/vue-forward-ref#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitscherlich/vue-forward-ref"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "default": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "dev": "vite example/ -c vite.config.js",
    "test": "vitest run",
    "release": "bumpp package.json && pnpm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^2.7 || ^3.0.0-0"
  },
  "dependencies": {
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "@m9ch/eslint-config-all": "0.3.2",
    "bumpp": "10.2.0",
    "eslint": "9.32.0",
    "typescript": "5.9.3",
    "unbuild": "^3.0.0",
    "vite": "7.0.6",
    "vitest": "3.2.4",
    "vue": "^3.4.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
